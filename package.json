        "nuxt:typecheck": "nuxi typecheck src",
        "nuxt:preview": "nuxi preview src",
        "nuxt:clean": "nuxi cleanup src",
        "nuxt:upgrade": "nuxi upgrade src --force",
        "dev": "nuxi dev src -o",
        "build:analyze": "nuxi build src --analyze",
        "build": "nuxi build src",
        "preview": "nuxi preview src",
        "prestart": "yarn nuxt:clean && yarn build",
