        "stylelint-check-scss": "stylelint ./src/**/*.scss",
        "stylelint-check-vue": "stylelint ./src/**/*.vue",
        "stylelint-check": "yarn stylelint-check-scss",
        "stylelint-fix-css": "stylelint ./src/**/*.css --fix",
        "stylelint-fix-scss": "stylelint ./src/**/*.scss --fix",
        "stylelint-fix-vue": "stylelint ./src/**/*.vue --fix",
        "stylelint-fix": "yarn stylelint-fix-scss",
        "eslint-check": "eslint --ext .js,.ts,.vue .",
        "eslint-fix": "eslint --fix --ext .js,.ts,.vue .",
        "lint": "yarn eslint-check && yarn stylelint-check",
        "lint:fix": "yarn eslint-fix && yarn stylelint-check",
        "pretest": "yarn lint",
        "next": "yarn version minor && yarn build && yarn generate",
        "next:major": "yarn version major && yarn build && yarn generate",
        "test:unit": "yarn vitest run --dir test",
        "test:e2e": "yarn vitest run --dir test",
        "nuxt:test:unit": "yarn nuxi test src",
        "nuxt:test:e2e": "yarn nuxi test src",
        "test": "jest"
    },
