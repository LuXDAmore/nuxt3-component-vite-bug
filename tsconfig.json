{
    "vueCompilerOptions": {
        "jsxTemplates": true,
        "experimentalRfc436": true
    },
    "compilerOptions": {
        "jsx": "preserve",
        "target": "ESNext",
        "module": "ESNext",
        "moduleResolution": "Node",
        "skipLibCheck": true,
        "strict": true,
        "allowJs": true,
        "noEmit": true,
        "resolveJsonModule": true,
        "allowSyntheticDefaultImports": true,
        "lib": [
            "esnext",
            "esnext.asynciterable",
            "dom"
        ],
        "esModuleInterop": true,
        "experimentalDecorators": true,
        "sourceMap": true,
        "noImplicitAny": false,
        "baseUrl": ".",
        "paths": {
            "~/*": [
                "./src/*"
            ],
            "~~/*": [
                "./*"
            ],
            "@/*": [
                "./src/*"
            ],
            "@@/*": [
                "./*"
            ],
            "#imports": [
                "src/.nuxt/imports"
            ],
        },
        "types": [
            "@types/node",
            "@nuxt/vue-app",
            "@types/lodash",
            "@pinia/nuxt"
        ]
    }
}
